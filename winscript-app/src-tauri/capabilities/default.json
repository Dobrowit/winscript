{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    "fs:allow-temp-write",
    "fs:allow-temp-read",
    "fs:allow-temp-write-recursive",
    "fs:allow-temp-read-recursive",
    "shell:allow-execute",
    "shell:allow-spawn",
    "shell:allow-open",
    "os:allow-hostname",
    "core:window:allow-set-effects",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "cmd",
          "cmd": "cmd.exe",
          "args": true
        }
      ]
    },
    "os:default"
  ]
}